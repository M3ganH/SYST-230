{
  "name": "Phish Hook",
  "version": "1.0.9",
  "description": "Beta Test warmup",
  "manifest_version": 3,
  "author": ["Christopher Ghanma", "Megan Hoxha", "Ronak Singh", "Eric Zipor", "Luke Wilson"],
  "icons": {
    "16": "logo.png", 
    "48": "logo.png", 
    "128": "logo.png" 
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Phish Hook"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab", "scripting"]
}
